/**
 * 8-way 2-bit multiplexor:
 * out = a if sel == 000
 *       b if sel == 001
 *       etc.
 *       h if sel == 111
 */

CHIP Mux8Way {
    IN a, b, c, d,
       e, f, g, h,
       sel[3];
    OUT out;

    PARTS:
	Mux4Way(a=a,b=b,c=c,d=d,sel[0]=sel[0],sel[1]=sel[1], out=mux4wayA);
	Mux4Way(a=e,b=f,c=g,d=h,sel[0]=sel[0],sel[1]=sel[1], out=mux4wayB);
	Mux(a=mux4wayA,b=mux4wayB,sel=sel[2],out=out);
}